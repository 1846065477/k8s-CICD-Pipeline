
echo | base64 -d > /opt/ca.crt
echo | base64 -d > /opt/client.crt
echo | base64 -d > /opt/client.key

openssl pkcs12 -export -out /opt/cert.pfx -inkey /opt/client.key -in /opt/client.crt -certfile /opt/ca.crt


